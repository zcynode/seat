<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jason.classroom.mapper.RoomMapper">

    <select id="selectAll" resultType="com.jason.classroom.entity.Room">
        select * from t_room
    </select>
    <select id="selectMaxId" resultType="java.lang.Integer">
        select max(id) from t_room
    </select>
</mapper>
